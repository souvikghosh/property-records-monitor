# Counties to monitor (comma-separated)
# Available: miami_dade, cook_county, maricopa, los_angeles
COUNTIES=miami_dade

# Search filters
# Price range (optional)
MIN_PRICE=100000
MAX_PRICE=500000

# Property types: residential, commercial, land, foreclosure, all
PROPERTY_TYPES=residential,foreclosure

# Areas/Zip codes to monitor (comma-separated, optional)
ZIP_CODES=33101,33132,33139

# Keywords in address or owner name (optional)
KEYWORDS=

# Notifications
DISCORD_WEBHOOK_URL=
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=
SMTP_PASSWORD=
NOTIFY_EMAIL=

# Webhook (POST JSON)
WEBHOOK_URL=

# Database
DATABASE_PATH=./data/properties.db

# Browser
HEADLESS=true
SCREENSHOT_ON_NEW=true
